<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.blackjack.gui.controllers.BlackjackSettingsController"
      spacing="15" alignment="CENTER" styleClass="root-pane">
    <padding><Insets top="20" right="20" bottom="20" left="20"/></padding>
    <Label text="⚙️ Blackjack Settings" styleClass="title-label"/>
